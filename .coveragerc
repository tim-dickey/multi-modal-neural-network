[run]
omit =
    tests/*
    src/__pycache__/*
    src/*/__pycache__/*
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:  # pragma: no cover
    @staticmethod
    @classmethod
    # typing or debug guards
    if TYPE_CHECKING:

[html]
directory = htmlcov
